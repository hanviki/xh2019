BODY
{
	font-size: 9pt;
}
table
{
	font-size: 9pt;
}
A:link
{
	color: #2f4f4f;
	text-decoration: none;
}
A:active {
	color: #555f69;
	text-decoration: none;
}
A:visited 
{
	color: #555f69;
	text-decoration: none;
}
A:hover {
	color: #990000;
	text-decoration: underline;/*A1A9AE*/
}
.td_separator {
}
.module_container
{
	font-size: 9pt;
	margin: 0px 5px 5px 0px;
	width: 100%;
	
	font-family: "宋体";
	background-image:url(../images/portal_insoffice/标题图.jpg);
	border-top-width: 1px;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-top-style: solid;
	border-right-style: solid;
	border-bottom-style: solid;
	border-left-style: solid;
	color: #1980A3;
	background-color: #1980A3;
	border-top-color: #1980A3;
	border-right-color: #1980A3;
	border-bottom-color: #1980A3;
	border-left-color: #1980A3;
}
.module_header
{
	overflow: hidden;
	width: 100%;
	height: 26px;
	font-family: "宋体";
	font-size: 9pt;
	font-weight: bold;	
	text-decoration: none;
	border-top-width: 0px;
	border-right-width: 0px;
	border-bottom-width: 1px;
	border-left-width: 0px;
	border-top-style: solid;
	border-right-style: solid;
	border-left-style: solid;
	color: #016087;
	border-top-color: #ffffff;
	border-right-color: #ffffff;
	border-bottom-color: #1980A3;
	border-left-color: #ffffff;
	filter:progid:DXImageTransform.Microsoft.Gradient(gradienttype=0, startcolorstr=#FFFFFF, endcolorstr=#51C4F7);
	text-indent: 1px;
	letter-spacing: 1px;
	word-spacing: 1px;
	border-bottom-style: solid;
	
}
.module_header_table
{
}
.module_header_setprefs
{
	cursor:hand;
	WIDTH: 30px; 
	border:0px;
	padding:0px;
	margin:0px;
	TEXT-ALIGN: center;	
	background-repeat: no-repeat;
}
.module_header_switch 
{
	cursor:hand;
	WIDTH: 16px; 
	TEXT-ALIGN: left;	
	background-image: url(images/summer/Portal/shou.jpg);
	background-repeat: no-repeat;
}
.module_header_switch_1 
{
	cursor:hand;
	WIDTH: 16px; 
	TEXT-ALIGN: left;	
	background-image: url(images/summer/Portal/fang.jpg);
	background-repeat: no-repeat;
}
.module_header_title {
	FONT-WEIGHT: bold;
	FONT-SIZE: 9pt;
	color: #143834;
	text-decoration: none;	
}
.module_header_refresh 
{
	cursor:hand;
	WIDTH: 16px;
	TEXT-ALIGN: center;
	background-image: url(images/summer/Portal/refrash.jpg);
	background-repeat: no-repeat;
}
.module_header_delete 
{
	cursor:hand;
	WIDTH: 16px;
	TEXT-ALIGN: center;
	background-image: url(images/summer/Portal/del.jpg);
	background-repeat: no-repeat;
}
.module_content
{
	background-color: #f5f5f5;
	overflow:auto;	
	height: 160px;
	scrollbar-arrow-color:#ffffff; /*三角箭头的颜色*/
　　scrollbar-base-color: #51C4F7;/*滚动条的基本颜色*/    
    scrollbar-highlight-color: #21A5C7;/*滚动条空白部分的颜色*/   
    scrollbar-darkshadow-color: #21A5C7; /*立体滚动条强阴影的颜色*/
    scrollbar-track-color: #21A5C7; /*立体滚动条背景颜色*/ 
    crollbar-shadow-color: #21A5C7; /*立体滚动条阴影的颜色*/
    
}
.module_more
{
	background-color:f5f5f5;
	height:5px;
	overflow:hidden;
}
.module_list
{
	font-size: 9pt;
	color: #778899;
	line-height: 18px;
	font-family: "宋体";
	text-decoration: underline;
	height:160px;
	overflow:auto;
}
td
{
	white-space:normal;
}